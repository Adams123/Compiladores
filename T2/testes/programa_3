PROGRAM TESTE6;
{teste de coment√°rio em programa}
TYPES	
	ArrayType ARRAY INTEGER[5];
	RecordType RECORD 
				STRING Name, LastName;
				ArrayType Grades;
			END;
VARIABLES 
	RecordType StudentRecord;
	STRING LastName;

BEGIN
	READ LastName;
	SET StudentRecord.Name = "Joe";
	SET StudentRecord.LastName = LastName;
	READ StudentRecord.Grades[1];
	WRITE StudentRecord.Grades[1];
END.
