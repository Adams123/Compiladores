{Testa as regras 0,1,2,3,4,5,11,12,13,14,15,16,20,21,22,23,24,25,26,27,28,30,31,32,33,34,35,36,37,38,40,41,42,44}
PROGRAM TESTE7;
FUNCTION media RETURNS REAL;
PARAMETERS
	INTEGER a;
	INTEGER b;
	INTEGER c;
VARIABLES 
	REAL SUM, AVERAGE;
BEGIN
	SET Sum = a+b+c;
	SET AVERAGE=SUM/3;
	RETURN (AVERAGE)
END;

{main}		 

BEGIN
	READ a;
	CALL FindFormula(a,b);
	WRITE b;
	READ N1;READ N2;READ N3;
	SET MediaAlunos = media(N1,N2,N3);
END.